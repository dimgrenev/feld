{
  "name": "Container",
  "description": "A container component that can hold other components and render them using a renderer function.",
  "properties": [
    { "name": "id", "type": "string", "description": "Unique identifier for the container" },
    { "name": "as", "type": "string", "description": "HTML element to render as", "defaultValue": "div" },
    { "name": "children", "type": "ComponentData[]", "description": "Child components to render", "required": true },
    { "name": "renderer", "type": "ComponentRenderer", "description": "Function to render child components", "required": true },
    { "name": "className", "type": "string", "description": "Additional CSS class names" }
  ],
  "events": []
} 